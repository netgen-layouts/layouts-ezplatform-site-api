{
    "name": "netgen/layouts-ezplatform-site-api",
    "description": "Netgen Layouts & eZ Platform Site API integration",
    "license": "GPL-2.0-only",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Netgen",
            "homepage": "https://www.netgenlabs.com"
        }
    ],
    "require": {
        "netgen/layouts-ezplatform": "~1.0.0",
        "netgen/ezplatform-site-api": "^2.6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "symfony/phpunit-bridge": "^4.2",
        "phpstan/phpstan": "^0.11",
        "phpstan/phpstan-strict-rules": "^0.11",
        "phpstan/phpstan-phpunit": "^0.11",
        "matthiasnoback/symfony-dependency-injection-test": "^4.0",
        "matthiasnoback/symfony-config-test": "^4.0.1",
        "franzl/studio": "*"
    },
    "scripts": {
        "test": "@php vendor/bin/phpunit --colors=always",
        "coverage": "@php -dzend_extension=xdebug.so vendor/bin/phpunit --coverage-html=coverage --colors=always",
        "phpstan": "@php vendor/bin/phpstan analyse -c phpstan.neon --level=7 --ansi lib bundle",
        "phpstan-tests": "@php vendor/bin/phpstan analyse -c phpstan.tests.neon --level=7 --ansi tests"
    },
    "autoload": {
        "psr-4": {
            "Netgen\\Layouts\\Ez\\SiteApi\\": "lib/",
            "Netgen\\Bundle\\LayoutsEzPlatformSiteApiBundle\\": "bundle/"
        }
    },
    "autoload-dev": {
        "files": [
            "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"
        ],
        "psr-4": {
            "Netgen\\Layouts\\Ez\\SiteApi\\Tests\\": "tests/lib/",
            "Netgen\\Bundle\\LayoutsEzPlatformSiteApiBundle\\Tests\\": "tests/bundle/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
